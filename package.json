{
	"name": "leetcode-problem-fetcher",
	"displayName": "LeetCode Problem Fetcher",
	"description": "Automatically fetch LeetCode problem descriptions and default Python3 code into your editor",
	"version": "1.0.4",
	"publisher": "nas03",
	"author": {
		"name": "nas03"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/nas03/leetcode-extension"
	},
	"bugs": {
		"url": "https://github.com/nas03/leetcode-extension/issues"
	},
	"homepage": "https://github.com/nas03/leetcode-extension#readme",
	"icon": "leetcode.png",
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Snippets",
		"Other"
	],
	"keywords": [
		"leetcode",
		"coding",
		"interview",
		"python",
		"algorithm",
		"competitive programming"
	],
	"activationEvents": [
		"onCommand:leetcode.fetchProblem"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "leetcode.fetchProblem",
				"title": "Fetch LeetCode Problem"
			}
		],
		"keybindings": [
			{
				"command": "leetcode.fetchProblem",
				"key": "ctrl+d ctrl+l",
				"mac": "cmd+d cmd+l",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"package": "vsce package",
		"publish": "vsce publish"
	},
	"devDependencies": {
		"@types/node": "^18.0.0",
		"@types/vscode": "^1.74.0",
		"@vscode/vsce": "^2.22.0",
		"typescript": "^4.9.4"
	},
	"dependencies": {
		"axios": "^1.6.0"
	}
}
